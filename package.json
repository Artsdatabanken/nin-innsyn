{
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "bugs": {
    "url": "https://github.com/Artsdatabanken/nin-kart-frontend/issues"
  },
  "dependencies": {
    "@artsdatabanken/typesystem": "^10.0.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@sentry/browser": "^4.6.4",
    "axios": "^0.18.0",
    "eslint-plugin-react-hooks": "^1.5.0",
    "leaflet": "^1.5.1",
    "react": "^16.8.4",
    "react-color": "^2.14.1",
    "react-dom": "^16.8.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "tangram": "^0.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.17"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": 2,
      "react-hooks/rules-of-hooks": 2,
      "jsx-a11y/alt-text": 1
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ]
  },
  "license": "MIT",
  "name": "nin-kart-frontend",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "eject": "react-scripts eject",
    "report-coverage": "coveralls < coverage/lcov.info",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test --coverage",
    "test:watch": "react-scripts test --watch"
  },
  "version": "0.2.0"
}
