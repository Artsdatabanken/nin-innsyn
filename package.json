    "react-test-renderer": "^16.3.2",
    "storybook-addon-material-ui": "^0.8.2"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!src/**/__tests__/*.{js}"
    ]
  }
