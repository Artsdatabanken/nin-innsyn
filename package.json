{
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "bugs": {
    "url": "https://github.com/Artsdatabanken/nin-innsyn/issues"
  },
  "dependencies": {
    "@artsdatabanken/typesystem": "^9.1.1",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@sentry/browser": "^4.6.3",
    "eslint-plugin-react-hooks": "^1.2.0",
    "leaflet": "^1.3.4",
    "react": "^16.8.3",
    "react-color": "^2.14.1",
    "react-dom": "^16.8.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "tangram": "^0.18.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.13",
    "@storybook/addon-links": "^4.1.13",
    "@storybook/addons": "^4.1.13",
    "@storybook/react": "^4.1.13",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.17"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": 2,
      "react-hooks/rules-of-hooks": 2
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ]
  },
  "license": "MIT",
  "name": "nin-innsyn",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "eject": "react-scripts eject",
    "report-coverage": "coveralls < coverage/lcov.info",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "react-scripts test --coverage",
    "test:watch": "react-scripts test --watch"
  },
  "version": "0.2.0"
}
