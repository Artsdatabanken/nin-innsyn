{
  "name": "ratatouille",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "deck.gl": "^5.1.4",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-typed": "^2.4.0",
    "graphql-request": "^1.4.0",
    "luma.gl": "^5.1.6",
    "material-ui": "^0.20.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "rc-slider": "^8.5.0",
    "react": "^16.2.0",
    "react-collapsible": "^2.0.4",
    "react-color": "^2.13.8",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-responsive": "^4.0.0",
    "react-router-dom": "^4.2.2",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "tangram": "^0.14.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --coverage",
    "test:watch": "node scripts/test.js --env=jsdom",
    "precommit": "pretty-quick --staged",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public -o build/storybook",
    "report-coverage": "coveralls < coverage/lcov.info"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-knobs": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/addon-storyshots": "^3.2.16",
    "@storybook/react": "^3.2.16",
    "coveralls": "^3.0.0",
    "flow-bin": "^0.71.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.4.1",
    "react-test-renderer": "^16.2.0",
    "storybook-addon-material-ui": "^0.8.2"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!src/**/__tests__/*.{js}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "coverageReporters": [
      "lcov"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
